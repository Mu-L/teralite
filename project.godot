[application]

config/name="Project Epsilon"
config/description="the new worst game ever made!"
run/main_scene="res://HUD and Menus/intro/intro.tscn"
config/use_custom_user_dir=true
boot_splash/image="res://HUD and Menus/splashscreen.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://HUD and Menus/Icons/epsilon_icon.png"
config/windows_native_icon="res://epsilon_icon.ico"

[audio]

default_bus_layout="res://Resources/Audio/default_bus_layout.tres"

[autoload]

global="*res://global.gd"

[debug]

settings/fps/force_fps=9999
gdscript/warnings/deprecated_keyword=false

